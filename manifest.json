{
	"manifest_version": 2,
	"name": "kc tier",
	"description": "it was kc tier and it was a chrome extension",
	"icons": {
		"16": "icon16.png",
		"64": "icon64.png"
	},
	"version": "1.4",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"background": {
		"scripts": [
			"background.min.js"
		],
		"persistent": false
	},
	"browser_action": {
		"default_icon": "icon16.png"
	},
	"permissions": [
		"storage",
		"*://*.krautchan.net/*"
	],
	"content_scripts": [
		{
			"matches": [
				"*://*.krautchan.net/*"
			],
			"js": [
				"kctier.min.js"
			],
			"run_at": "document_start"
		}
	]
}