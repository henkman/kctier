{
	"manifest_version" : 2,
	"name" : "kc tier",
	"description" : "it was kc tier and it was an extension",
	"icons": {
		"16": "icon16.png",
		"64": "icon64.png"
	},
	"version" : "1.0",
	"options_ui": {
		"page": "options.html",
		"chrome_style": true
	},
	"browser_action" : {
		"default_icon" : "icon16.png"
	},
	"permissions" : [
		"storage",
		"*://*.krautchan.net/*"
	],
	"content_scripts" : [
		{
			"matches" : ["*://*.krautchan.net/*"],
			"js" : ["kctier.min.js"]
		}
	]
}
